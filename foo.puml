@startuml
    [*] -> A
    state A {
        note "B" as B
        [*] -> C
        state C
        state D
        C -> D: a / x
        --
        note "E" as E
        [*] -> F
        state F
        state G
        state H <<choice>>
        F -> H: a / y
        H -> F: [ok] 
        H -> G: [else]
        --
        state W : label
    }
@enduml
